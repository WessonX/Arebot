<launch>
    <!-- 配置日志文件，只输出想看的 -->
    <env name = "ROSCONSOLE_CONFIG_FILE" value = "$(find path_plan)/rosconsole.config" />

    <!-- 1.SLAM实现 -->
    <include file = "$(find path_plan)/launch/slam.launch"/>

    <!-- 2.导航中的move_base -->
    <include file = "$(find path_plan)/launch/move_base.launch"/>

    <!-- 3.启动路径记录节点 -->
    <node pkg="path_plan" type="pathRecord_node" name="pathRecord_node" output="screen" >
        <param name="path" value="/home/huzife/arebot_ws/src/path_plan/path_plan/path.txt"/>
    </node>

</launch>
